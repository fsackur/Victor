{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug C#",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "pwsh",
            "args": [
                "-NoExit",
                "-NoProfile",
                "-Command",
                "Import-Module '${workspaceFolder}/Build/Victor'"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "justMyCode": true,
            "enableStepFiltering": false,   // step into properties
            "logging": {
                "moduleLoad": false
            }
        }
    ]
}
