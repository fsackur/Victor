{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "dotnet-publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Victor/Victor.csproj",
                "-c", "Debug"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build",
            "type": "shell",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "command": "Invoke-Build Build",
            "group": {
                "kind": "build"
            }
        }
    ]
}
